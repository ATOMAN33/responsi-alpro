	cout << "|Lokasi : ";
	cin >> lokasi;
	cout << "|Fasilitas : ";
	cin >> fasilitas;
	cout << "|==========================================|" << endl;
	cout << " " << endl;
	cout << " " << endl;
	cout << "|=================================|" << endl;
	cout << kapasitas << "," << lokasi << "," << fasilitas << endl;
	cout << "|=================================|" << endl;
	cout << " " << endl;
	if (admin == 1, pengunjung == 0) {
		cout << "DIUBAH" << endl;
		cout << " " << endl;
}else{
		cout << "TIDAK DIUBAH" << endl;
		cout << " " << endl;
	}
	
	cout << "|==========================================|"<<endl;
	cout << "Matakuliah: ";
