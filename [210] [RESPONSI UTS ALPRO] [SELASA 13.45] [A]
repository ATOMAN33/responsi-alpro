	cin >> butuh;
	cout << "|==========================================|"<<endl;
	cout << " " << endl;
	cout << " " << endl;
	cout << "|=================================================================|" << endl;
	cout << matkul << "," << dosen << "," << waktu << "," << butuh << endl;
	cout << "|=================================================================|" << endl;
	
	
	cout << "LOGIN SIMERU" << endl;
	user = mhs;

	pw = 1995;
	cout<<"masukkkan username: ";
	cin>>user;
	cout<<"masukkan password: ";
	cin>>pw;
	
	if (user , pw){
		cout << "PENGUNJUNG"<<endl;
	}else {
		cout<<"ADMIN"<<endl;
	}
