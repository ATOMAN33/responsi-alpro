#include <iostream>
using namespace std;

int main() {
	int password;
	int kapasitas;
	string waktu;
	int pw;
	
	string username;
	string fasilitas;
	string matkul;
	string dosen;
	string butuh;
	string user;
	
	string lokasi;
	char mhs;
	char admin;
	char pengunjung;
	
	username = admin;
	password = 1994;
	cout << "|=============================|" << endl;
	cout << "|Masukkan username : ";
	cin >> username;
	cout << "|Masukkan pasword : ";
	cin >> password;
	cout << "|=============================|" << endl;
	
	
	if (username , password ){
		cout << " " << endl;
		cout << " " << endl;
		cout << "ADMIN" << endl;
		cout << " " << endl;
		cout << " " << endl;
	}else{
		cout << " " << endl;
		cout << " " << endl;
		cout << "PENGUNJUNG" << endl;
		cout << " " << endl;
		cout << " " << endl;
	}
	cout << "|==========================================|"<<endl;
